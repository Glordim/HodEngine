{
	"version": 10,
	"configurePresets": [
		{
			"name": "toolchain-windows-clang-x64",
			"hidden": true,
			"condition": {
				"type": "equals",
				"lhs": "${hostSystemName}",
				"rhs": "Windows"
			},
			"toolchainFile": "${sourceDir}/CMake/Toolchains/Windows-ClangCl-x64.cmake"
		},
		{
			"name": "toolchain-windows-msvc-x64",
			"hidden": true,
			"condition": {
				"type": "equals",
				"lhs": "${hostSystemName}",
				"rhs": "Windows"
			},
			"toolchainFile": "${sourceDir}/CMake/Toolchains/Windows-Msvc-x64.cmake",
			"architecture": {
				"value": "x64",
				"strategy": "external"
			}
		}
	]
}